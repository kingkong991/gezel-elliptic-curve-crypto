#include "constants.inc"

#include "mult_mod.fdl"

dp testbench {
	sig A: ns(const_m);
	sig B: ns(const_m);
	sig P: ns(const_m);

	sig T: ns(const_m);

	sig start: ns(1);

	use montgomery(A, B, P, T, start);

	sfg run {
		A = 1;
		B = 3;
		P = 5;

		start = 1;

		$display("[Cycle ", $cycle, "]");
	}
} hardwired h_testbench (testbench) { run; }

system S {
	testbench;
}

